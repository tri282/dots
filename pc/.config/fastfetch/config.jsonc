{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "source": "~/.config/fastfetch/mu.txt",
    "padding": {
      "right": 0,
      "top": 0
    },
    "color": {
      "1": "red",
      "2": "black",
      "3": "magenta",
     }
  },
   "general": {
    "multithreading": true
  },
  "display": {
    "separator": "➜   "
  },
  "modules": [
    "break",
    {
      "type": "title",
      "format": "                                {6}{7}{8}"
    },
    "break",
    {
      "type": "custom",
      "format": "┌──────────────────────────────────────────────────────────────────────────────┐" // `\u001b` is `\033`, or `\e`
    },
    "break",
    {
      "key": "     OS           ",
      "keyColor": "38;2;135;67;71",
      "type": "os"
    },
    {
      "key": "    󰌢 Machine      ",
      "keyColor": "38;2;215;155;160",
      "type": "host"
    },
    {
      "key": "     Kernel       ",
      "keyColor": "38;2;135;67;71",
      "type": "kernel"
    },
    {
      "key": "    󰅐 Uptime       ",
      "keyColor": "38;2;215;155;160",
      "type": "uptime"
    },
    {
      "key": "     Packages     ",
      "keyColor": "38;2;135;67;71",
      "type": "packages"
    },
    {
      "key": "     WM           ",
      "keyColor": "38;2;215;155;160",
      "type": "wm"
    },
    {
      "key": "     Shell        ",
      "keyColor": "38;2;135;67;71",
      "type": "shell"
    },
    {
      "key": "     Terminal     ",
      "keyColor": "38;2;215;155;160",
      "type": "terminal"
    },
    {
      "key": "     Font         ",
      "keyColor": "38;2;135;67;71",
      "type": "terminalfont"
    },
    {
      "key": "    󰻠 CPU          ",
      "keyColor": "38;2;215;155;160",
      "type": "cpu"
    },
    {
      "key": "    󰍛 GPU          ",
      "keyColor": "38;2;135;67;71",
      "type": "gpu"
    },
    {
      "key": "    󰑭 Memory       ",
      "keyColor": "38;2;215;155;160",
      "type": "memory"
    },
    {
      "key": "     Wifi         ",
      "keyColor": "38;2;135;67;71",
      "type": "wifi"
    },
    {
      "key": "    󰩟 Local IP     ",
      "keyColor":  "38;2;215;155;160",
      "type": "localip",
      "compact": true
    },
    "break",
    {
      "type": "custom",
      "format": "└──────────────────────────────────────────────────────────────────────────────┘" // `\u001b` is `\033`, or `\e`
    },
    "break",
    {
      "type": "colors",
      "paddingLeft": 34,
      "symbol": "circle",
      "block": {
        "width": 10
      }
    }
  ]
}
